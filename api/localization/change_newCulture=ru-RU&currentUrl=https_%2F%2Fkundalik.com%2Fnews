        require(['ab/ab'], function (ab) {
            window.onload = function () {
                var metrikaIds = dnevnik.settings.yandexMetrikaIds;
                metrikaIds.forEach(metrikaId => {
                    if (!window['yaCounter' + metrikaId]) {
                        console.debug('Yandex.Metrika with id ' + metrikaId + ' not loaded!');
                    }
                })
                
